[General]
# 要开MPV的话必须添加
force-http-engine-hosts = aliyun.example.com:0, quark.example.com:0, pikpak.example.com:0

[Script]
# 阿里云盘、夸克网盘和PikPak网盘挂载
http-request ^http:\/\/(aliyun|quark|pikpak)\.example\.com script-path = https://raw.githubusercontent.com/Moli-X/Tool/X/Loon/Script/lodepuly/Fileball/Fileball_mount.js, requires-body = true, tag = 三合一网盘挂载