[Argument]
token = input, "", tag = csrfToken, desc = 这里需要输入csrfToken的值
cookie = input, "", tag = Cookie, desc = 这里需要输入Cookie的值
cron = input, "0 1 * * *", tag = 定时参数, desc = 这里需要输入符合CRON表达式的参数

[Script]
# 定时签到
cron {cron} script-path = https://raw.githubusercontent.com/Moli-X/Tool/X/Loon/Script/lodepuly/FollowRSS/FollowRSS_checkin.js, tag = Follow每日签到, timeout = 120, argument=[{token},{cookie}]